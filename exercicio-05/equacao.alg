algoritmo equação 

    real a, b, c, x1, x2
    
    início
        escreva "Digite o valor de a"
        leia a
        escreva "Digite o valor de b"
        leia b
        escreva "Digite o valor de c"
        leia c
        x1=(-b+sqrt(b^2-4*a*c))/(2*a)
        x2=(-b-sqrt(b^2-4*a*c))/(2*a)
        escreva "x1 é igual a:" + x1 + "e x2 é igual a:" + x2
    fim