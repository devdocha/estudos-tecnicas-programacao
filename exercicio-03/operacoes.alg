algoritmo operações

        inteiro num1, num2

    início
        escreva "Digite um número"
        leia num1
        escreva "Digite outro número"
        leia num2
        escreva "Soma:" + num1+num2
        escreva "Subtração:" + num1-num2
        escreva "Multiplicação:" + num1*num2
        escreva "Divisão:" + num1/num2
        escreva "Resto:" + num1%num2
    fim